(mac . '(l r)
	(js! memberExpression ,l ,r false))

(mac list '()
	(js! arrayExpression ,((. (. (Array) slice) call) arguments)))

(mac 位 '(args body)
	(js! functionExpression
		null
		,((. args map) (js! identifier))
		(js! blockStatement
			,(list (js! returnStatement ,body)))))

(mac 位 '(name args body)
	(js! functionDeclaration
		,name
		,((. args map) (js! identifier))
		(js! blockStatement
			,(list (js! returnStatement ,body)))))

(位 '(a) a)
(位 id '(a) a)